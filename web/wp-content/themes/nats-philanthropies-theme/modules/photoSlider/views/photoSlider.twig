{% block photoSlider %}
<div class="{{ classes }}">
    <div class="l-container photoSlider-inner">
        <div class="photoSlider-images" data-slider-type="{{ slider_type }}" id="{{ unique_id }}">
             {% for image in images %}
                <img src="{{ image }}" alt="">
            {% endfor %}
        </div>

    </div>
</div>
<script type="text/javascript">
    var is_multiple = {{ is_multiple }};
</script>
{% endblock %}
